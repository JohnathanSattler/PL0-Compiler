const val = 10;
var num;

procedure d;
	var i;

	begin
		i := num;
		while i < val do begin
			write i;
			i := i + 1;
		end;
	end;

procedure a;
	const val = 7;
	var num;

	procedure b;
		procedure c;
			begin
				num := num + 1;
				call d;
			end;

		begin
			num := num + 1;
			call c;
		end;

	begin
		num := 2;
		call b;
		write val;
	end;

begin
	num := 1;
	call a;
	write val;
	write num;
end.